{
  "tasks": {
    "dev": "deno run -A --unstable --watch=static/,src/ main.ts --dev",
    "start": "deno run --unstable --allow-net --allow-read main.ts"
  },
  "imports": {
    "marked": "npm:marked@11.1.0",
    "nano-jsx": "https://deno.land/x/nano_jsx@v0.1.0/mod.ts",
    "oak": "https://deno.land/x/oak@v12.6.1/mod.ts",
    "twind-core": "https://esm.sh/@twind/core@1.1.3",
    "twind-preset-tailwind": "https://esm.sh/@twind/preset-tailwind@1.1.4"
  },
  "fmt": {
    "semiColons": false,
    "singleQuote": true,
    "indentWidth": 2,
    "useTabs": false
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns",
      "deno.unstable"
    ],
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "noImplicitAny": false
  }
}